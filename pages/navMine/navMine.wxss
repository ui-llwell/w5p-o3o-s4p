/* pages/navMine/navMine.wxss */
page {
  width: 750rpx;
  height: 100%;
  background: #F4F4F4;
}
.header {
  width: 750rpx;
  height: 623rpx;
  overflow: hidden;
}
.header_top {
  width: 750rpx;
  height: 391rpx;
  background: #f4f4f4;
}
.header_bottom {
  width: 750rpx;
  height: 230rpx;
  background: #fff;
  position: relative;
  border-bottom: 1rpx solid #E5E5E5;
}
.header_img {
  width: 240rpx;
  height: 240rpx;
  border-radius: 50%;
  display: block;
  overflow: hidden;
  position: absolute;
  left: 50%;
  top:-120rpx;
  margin-left: -120rpx;
}
.header_name {
  width: 750rpx;
  font-family: PingFangSC-Regular;
  font-size: 40rpx;
  color: #000000;
  text-align: center;
  padding-top: 130rpx;  
}

.main_nav {
  width: 690rpx;
  height: 89rpx;
  background: #fff;
  padding: 0 30rpx;
  line-height: 89rpx;
  position: relative;
}
.main_nav text {
  display: block;
  float: left;
  height: 89rpx;
}
.main_title {
  width: 200rpx;
  font-size: 34rpx;
  color: #000000;
 
}
.main_theme {
  width:430rpx;
  font-size: 34rpx;
  color: #888;
  display: block;
  text-align: right;
  margin-right: 30rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  }
.main_logo {
  font-size: 34rpx;
  color: #C7C7CC;
  text-align: right;
}
.content {
  width: 750rpx;
  height: 193rpx;
  background: #fff;
  display: flex;
}
.content_wrap {
  width: 90rpx;
  height: 150rpx;
  flex: 1;
  margin-top: 35rpx;
}
.content_wrap image {
  width: 80rpx;
  height: 80rpx;
  display: block;
  margin: 0 auto;
}
.content_text {
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #5E5E5E;
  margin-top: 16rpx;
}
.mar {
  margin-top: 20rpx;
}
.line {
  margin-top: 1rpx;
}
 .footer {
   width: 750rpx;
   height: 127rpx;
 } 
 .footer image {
   width: 470rpx;
   height: 80rpx;
   margin: 30rpx auto 0;
   display: block;
